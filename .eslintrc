{
  "extends": "airbnb-base",
  "ecmaFeatures": {
    "modules": true
  },
  "env": {
    "browser": false,
    "node": true
  },
  "rules": {
    "no-underscore-dangle": [ "error", { "allowAfterThis": true } ],
    "comma-dangle": [ "error", "never" ],
    "prefer-template": 0,
    "no-param-reassign": 0,
    "import/no-extraneous-dependencies": [
      "error",
      { "devDependencies": [ "**/test/*.js", "**/test/**/*.js" ] }
    ]
  }
}
